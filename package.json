{
  "name": "web-client",
  "version": "1.0.0",
  "description": "Boilerplate ReactRedux App",
  "main": "src/index.js",
  "scripts": {
    "prepush": "yarn run check",
    "precommit": "lint-staged",
    "clean": "rimraf dist/*",
    "build:webpack": "cross-env NODE_ENV=${NODE_ENV:-production} STAGE=development webpack",
    "build:webpack:prod": "cross-env NODE_ENV=${NODE_ENV:-production} STAGE=production webpack",
    "build:webpack:staging": "cross-env NODE_ENV=${NODE_ENV:-production} STAGE=staging webpack",
    "build:staging": "yarn run clean && yarn run build:webpack:staging",
    "build:production": "yarn run clean && yarn run build:webpack:prod",
    "start": "webpack-dev-server --progress --colors --config ./webpack.config.js --host 0.0.0.0",
    "start:prod": "node static-server.js",
    "lint": "eslint --ext .js,.jsx src",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "check": "yarn run test"
  },
  "browserslist": [
    "> 1%",
    "last 8 versions"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/testSetup/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|svg)$": "<rootDir>/src/testSetup/testMockData/styleMock.js",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^lib(.*)$": "<rootDir>/src/lib$1",
      "^testSetup(.*)$": "<rootDir>/src/testSetup$1",
      "^src(.*)$": "<rootDir>/src$1"
    },
    "transformIgnorePatterns": []
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add",
      "yarn run lint"
    ]
  },
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^0.18.1",
    "classnames": "^2.2.3",
    "compression": "^1.7.2",
    "cookies-js": "^1.2.2",
    "cors": "^2.8.4",
    "ejs": "^2.5.8",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
