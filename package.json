    "lodash": "^4.17.14",
    "moment": "^2.12.0",
    "moment-range": "^4.0.1",
    "normalize.css": "^4.2.0",
    "normalizr": "^3.3.0",
    "papaparse": "^4.5.0",
    "qs": "^6.5.0",
    "rc-slider": "^8.6.5",
    "rc-tooltip": "^3.7.0",
    "react": "^16.8.6",
